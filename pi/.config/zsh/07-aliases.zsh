# Environment
alias reloadzsh='source $HOME/.zshenv && source $ZDOTDIR/.zshrc'
alias dots='cd $DOTFILES'
alias backup='git_world_backup'
alias updt='sudo apt update && sudo apt upgrade'

# Directories
alias la='ls -lAh'
alias ll='ls -lh'

# Vim
alias v='nvim'
alias vim='nvim'

# Copy and move
alias cp='cp -iv'
alias mv='mv -iv'

# Navigation
alias .2='../..'
alias .3='../../..'
alias .4='../../../..'

